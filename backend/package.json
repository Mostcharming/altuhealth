{
  "name": "altu-health-backend",
  "version": "1.0.0",
  "description": "Altu Health ERP Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.js'",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:prod": "pm2 start ecosystem.config.js --env production --update-env",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js --env production --update-env",
    "pm2:reload": "pm2 reload ecosystem.config.js --env production --update-env",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "erp",
    "health",
    "hmo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "pm2": "^5.2.0",
    "sequelize-cli": "^6.6.3"
  }
}
